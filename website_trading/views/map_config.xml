<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="google_map_config" model="ir.ui.view">
        <field name="name">Google Map Config</field>
        <field name="model">website.config.settings</field>
        <field name="inherit_id" ref="website.view_website_config_settings"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='social']" position="after">
                <group name="map" string="Google Map Embed Url">
                    <label for="map_url" string="Map Url"/>
                        <div>
                            <div id="map_key" class="oe_inline">
                                <field name="map_key" placeholder="GoogleMap Embed Url."/>
                            </div>
                        </div>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
